{
  "name": "super-search-hub",
  "version": "1.12.7",
  "author": "CaoMeiYouRen",
  "description": "将任何能搜索的网页的搜索结果处理成一种统一的格式",
  "main": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/bin/www.ts",
    "dev:webpack": "cross-env NODE_ENV=development webpack --config ./webpack.config.js && npm run start",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "prebuild": "rimraf dist",
    "docs:dev": "nodemon --ext md,vue,js,css --watch docs --exec vuepress dev docs",
    "docs:dev2": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "predocs:build": "rimraf public && npm run docs:changelog",
    "docs:changelog": "conventional-changelog -p cmyr-config -i docs/changelog/README.md -s -r 0",
    "rm": "rimraf node_modules",
    "debug": "cross-env DEBUG=koa:* npm run dev",
    "commit": "git add . && git cz && git pull && git push",
    "precommit": "npm run lint",
    "changelog": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "release": "semantic-release",
    "test": "mocha -r ts-node/register test/register.ts src/**/*.test.ts test/**/*.test.ts -R nyan -t 10000",
    "posttest": "npm run test:webpack",
    "test:unit": "mocha -r ts-node/register test/register.ts src/**/*.test.ts -R nyan -t 10000",
    "test:e2e": "mocha -r ts-node/register test/register.ts test/**/*.test.ts -R nyan -t 10000",
    "test:nyc": "rimraf coverage && nyc --reporter=lcov --reporter=text mocha -r ts-node/register test/register.ts src/**/*.test.ts test/**/*.test.ts -R nyan -t 10000",
    "test:webpack": "rimraf dist && cross-env NODE_ENV=development webpack --config ./webpack.config.js && cross-env MODE_ENV=test CACHE_TYPE=memory npm run start",
    "coverage": "codecov",
    "precoverage": "npm run test:nyc"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/super-search-hub.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/super-search-hub/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/super-search-hub#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "commit-msg": "validate-commit-msg"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "8.0.8",
    "@types/koa-static": "^4.0.1",
    "art-template": "CaoMeiYouRen/art-template",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "dayjs": "^1.9.7",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "file-stream-rotator": "^0.5.7",
    "fs-extra": "^9.0.1",
    "git-rev-sync": "^3.0.1",
    "http-status": "^1.5.0",
    "iconv-lite": "^0.6.2",
    "ioredis": "^4.19.2",
    "ip-address": "CaoMeiYouRen/ip-address",
    "koa": "^2.13.0",
    "koa-art-template": "^1.1.1",
    "koa-bodyparser": "^4.3.0",
    "koa-cache-control": "^2.0.0",
    "koa-log4": "^2.3.2",
    "koa-static": "^5.0.0",
    "koa2-ratelimit": "CaoMeiYouRen/koa2-ratelimit",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "mime": "^2.5.2",
    "mockjs": "^1.1.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "pidusage": "^2.0.21",
    "query-string": "^6.13.7",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.9.0",
    "ts-dingtalk-robot": "^1.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/cheerio": "^0.22.23",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.4",
    "@types/git-rev-sync": "^2.0.0",
    "@types/ioredis": "^4.17.8",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cache-control": "^2.0.1",
    "@types/koa-log4": "^2.3.2",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "8.0.3",
    "@types/lodash": "^4.14.165",
    "@types/lru-cache": "^5.1.0",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^8.2.0",
    "@types/mockjs": "^1.0.3",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^15.12.3",
    "@types/pidusage": "^2.0.1",
    "@types/supertest": "^2.0.10",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@vuepress/plugin-active-header-links": "^1.7.1",
    "@vuepress/plugin-back-to-top": "^1.7.1",
    "@vuepress/plugin-google-analytics": "^1.7.1",
    "@vuepress/plugin-nprogress": "^1.7.1",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-cmyr-config": "^1.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "entities": "^2.1.0",
    "eslint": "^7.15.0",
    "eslint-config-cmyr": "^1.0.3",
    "husky": "^4.3.8",
    "markdown-it": "^12.0.3",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "semantic-release": "^17.3.0",
    "semantic-release-docker": "^2.2.0",
    "should": "^13.2.3",
    "supertest": "^6.0.1",
    "ts-node-dev": "^1.0.0",
    "validate-commit-msg": "^2.14.0",
    "vuepress": "^1.7.1"
  }
}
